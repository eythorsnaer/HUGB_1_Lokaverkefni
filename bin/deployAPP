#!/bin/bash

APP="TicTacToe-SoftwareBoys"

# Always deploy new version, clean before
bin/clean
bin/packageAPP

# If the installation folder exists under ~/tmpAPP, remove it
test -f ~/tmpAPP/${APP} && rm -r /tmpAPP/${APP}
cp -r build/install/${APP} ~/tmpAPP

# Run application once
~/tmpAPP/${APP}/bin/${APP}
